               RowBox[{"i", ",", "1", ",", "d"}], "}"}]}], "]"}]}], " ", 
           "}"}], "]"}], ",", 
         RowBox[{"vars", "[", "t", "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "t1", ",", "t2"}], "}"}], ",", 
         RowBox[{"Method", "\[Rule]", "\"\<StiffnessSwitching\>\""}]}], 
