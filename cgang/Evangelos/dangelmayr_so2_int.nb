Cell[BoxData[
 RowBox[{
  RowBox[{"reqbPar", "[", 
   RowBox[{
   "m1_", ",", "m2_", ",", "c1_", ",", "c2_", ",", "a1_", ",", "b1_", ",", 
    "a2_", ",", "b2_", ",", "e2_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"sol", ",", "A1", ",", "A2", ",", "w"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"A1", "[", 
       RowBox[{"u_", ",", "v_"}], "]"}], ":=", 
      RowBox[{"m1", "+", 
       RowBox[{"a1", " ", "u"}], "+", 
       RowBox[{"b1", " ", "v"}]}]}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"A2", "[", 
