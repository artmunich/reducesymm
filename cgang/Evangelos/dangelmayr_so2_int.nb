 RowBox[{
  RowBox[{"reqbEqPar", "[", 
   RowBox[{
   "u_", ",", "v_", ",", "m1_", ",", "m2_", ",", "c1_", ",", "c2_", ",", 
    "a1_", ",", "b1_", ",", "a2_", ",", "b2_", ",", "e2_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"eqs", ",", "A1", ",", "A2"}], "}"}], ",", 
    RowBox[{
     RowBox[{"A1", "=", 
      RowBox[{"m1", "+", 
       RowBox[{"a1", " ", "u"}], "+", 
       RowBox[{"b1", " ", "v"}]}]}], ";", " ", 
     RowBox[{"A2", "=", 
      RowBox[{"m2", "+", 
       RowBox[{"a2", " ", "u"}], "+", 
       RowBox[{"b2", " ", "v"}]}]}], ";", " ", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"c2", " ", "u", " ", "A1"}], "-", 
        RowBox[{"c1", " ", "v", " ", "A2"}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"A1", "^", "2"}], "/", 
            SuperscriptBox["c1", "2"]}], "-", " ", "v"}], ")"}], " ", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"c2", " ", "u"}], "+", 
            RowBox[{"2", " ", "c1", " ", "v"}]}], ")"}], "^", "2"}]}], "+", 
        RowBox[{
         RowBox[{"e2", "^", "2"}], " ", 
         RowBox[{"v", "^", "2"}]}]}], ",", 
       RowBox[{
        RowBox[{"4", 
         RowBox[{"A1", "^", "2"}], " ", 
         RowBox[{
          RowBox[{"u", "^", "2"}], "/", 
          RowBox[{"c1", "^", "2"}]}]}], "+", 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox[
           RowBox[{"2", "u", " ", "v", " ", "e2"}], 
           RowBox[{
            RowBox[{"2", "c1", " ", "v"}], "+", 
            RowBox[{"c2", " ", "u"}]}]], ")"}], "2"], "-", 
        RowBox[{"4", 
         SuperscriptBox["u", "2"], "v"}]}]}], "}"}]}]}], " ", 
   "]"}]}]], "Input",
